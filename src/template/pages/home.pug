												g(stroke='none' stroke-width='1' fill='none' fill-rule='evenodd')
													g(fill='#D3D3D3' fill-rule='nonzero')
														path(d='M7,0 C3.13403326,0 0,3.13397236 0,7 C0,13.0000005 7,18.375 7,18.375 C7,18.375 14,13.0000005 14,7 C14,3.13397236 10.8659663,0 7,0 Z M7,10.5 C5.06699594,10.5 3.5,8.93300406 3.5,7 C3.5,5.06699594 5.06699594,3.5 7,3.5 C8.93300406,3.5 10.5,5.06699638 10.5,7 C10.5,8.93300362 8.93300406,10.5 7,10.5 Z')
											span= item.place
									if item.actLink
										+e.item-action
											+e.A(href='#!')= item.actLink

						-n = 0
						while n < 5
							.c-tabs__tab
								+e.tabs-inner
									+e.grid-sizer
									-
										eventsList = [{
										label: 'FITNESS CAMP',
										labelClass: '_2',
										img: 'event_img',
										descr: '7-дневный фитнес лагерь в Греции с Анитой и Юлей',
