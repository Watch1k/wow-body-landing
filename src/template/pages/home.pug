					+social('_white program-card__socials', 'fb', 'inst')
				+e.info
					+e.info-content
						p Cоздание рельефного тела, плоского живота и тонкой талии

						+e.info-data
							+e.info-data-item
								span 4
								p недели
							+e.info-data-item
								span 242
								p упражнения
							+e.info-data-item
								span 459
								p рецептов
							+e.info-data-item
								span €14
								p стоимость

						+e.A.info-btn.btn._lg._orange(href='#!')
							span УЗНАТЬ БОЛЬШЕ О ПРОГРАММЕ

			+b.program-card._heavy.is-disabled
				+e.mask.decor-mask
				+e.inner
					+e.IMG.img(src='static/img/bg/product_heavy.png' alt='image')
					+e.back-title HEAVY
					+e.content
						+e.A.logo-link(href='#!')
