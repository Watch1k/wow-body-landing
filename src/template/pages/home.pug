extends ../extend/default

block variables
	- var title = 'Home page';

block main
	main.content(data-page='home')

		//begin programs
		+b.SECTION.programs#programs

			+b.program-card._mom.is-disabled
				+e.mask.decor-mask
				+e.inner
					+e.IMG.img(src='static/img/bg/product_mom.png' alt='image')
					+e.back-title MOM
					+e.content
						+e.A.logo-link(href='#!')
							+e.IMG(src='static/img/logo/wow-mom.png' width='216' height='25' alt='image')
						p Твой гид по здоровой и красивой беременности!
					+social('_white program-card__socials', 'fb', 'inst')
				+e.info
					+e.info-content
						p Твой гид по здоровой и красивой беременности!
						+e.info-data
							+e.info-data-item
								span 1
								p семестр
							+e.info-data-item
								span 242
								p упражнения
							+e.info-data-item
								span 459
								p рецептов
							+e.info-data-item
								span ₴494
								p стоимость

						+e.A.info-btn.btn._lg._blue(href='#!')
							span УЗНАТЬ БОЛЬШЕ О ПРОГРАММЕ

			+b.program-card._slim.is-disabled
				+e.mask.decor-mask
				+e.inner
					+e.IMG.img(src='static/img/bg/product_slim.png' alt='image')
					+e.back-title SLIM
					+e.content
						+e.A.logo-link(href='#!')
							+e.IMG(src='static/img/logo/wow-slim.png' width='212' height='25' alt='image')
						p Тренировки, питание,  советы и мотивация. 84 дня новой жизни.
					+social('_white program-card__socials', 'fb', 'inst')
				+e.info
					+e.info-content
						p Тренировки, питание,  советы и мотивация. 84 дня новой жизни.

						+e.info-data
							+e.info-data-item
								span 12
								p недель
							+e.info-data-item
								span 242
								p упражнения
							+e.info-data-item
								span 459
								p рецептов
							+e.info-data-item
								span €84
								p стоимость

						+e.A.info-btn.btn._lg._pink-2(href='#!')
							span УЗНАТЬ БОЛЬШЕ О ПРОГРАММЕ

			+b.program-card._strong.is-disabled
